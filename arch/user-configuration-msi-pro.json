{
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "custom-commands": [
        "curl --location --silent 'https://raw.githubusercontent.com/asininemonkey/bootstrap/main/arch/aur.sh' | bash",
        "curl --location --silent 'https://raw.githubusercontent.com/asininemonkey/bootstrap/main/arch/common.sh' | bash",
        "curl --location --silent 'https://raw.githubusercontent.com/asininemonkey/bootstrap/main/arch/dotfiles.sh' | bash",
        "curl --location --silent 'https://raw.githubusercontent.com/asininemonkey/bootstrap/main/arch/gnome.sh' | bash",
        "curl --location --silent 'https://raw.githubusercontent.com/asininemonkey/bootstrap/main/arch/msi-pro-custom.sh' | bash"
    ],
    "disk_layouts": "/tmp/user-disk-layouts.json",
    "harddrives": [
        "/dev/nvme0n1"
    ],
    "hostname": "msi-pro",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "uk",
    "mirror-region": {
        "United Kingdom": {
            "https://archlinux.uk.mirror.allworldit.com/archlinux/$repo/os/$arch": true,
            "https://lon.mirror.rackspace.com/archlinux/$repo/os/$arch": true,
            "https://mirror.bytemark.co.uk/archlinux/$repo/os/$arch": true,
            "https://mirror.cov.ukservers.com/archlinux/$repo/os/$arch": true,
            "https://mirrors.melbourne.co.uk/archlinux/$repo/os/$arch": true,
            "https://www.mirrorservice.org/sites/ftp.archlinux.org/$repo/os/$arch": true
        }
    },
    "nic": {
        "NetworkManager": true
    },
    "ntp": false,
    "packages": [
        "archlinux-wallpaper",
        "baobab",
        "btop",
        "celluloid",
        "code",
        "cups",
        "dconf-editor",
        "docker",
        "docker-compose",
        "eog",
        "eog-plugins",
        "evince",
        "exfat-utils",
        "fail2ban",
        "firefox-i18n-en-gb",
        "flatpak",
        "foliate",
        "fprintd",
        "gamemode",
        "gdm",
        "geary",
        "git",
        "globalprotect-openconnect",
        "gnome-backgrounds",
        "gnome-boxes",
        "gnome-calculator",
        "gnome-calendar",
        "gnome-characters",
        "gnome-control-center",
        "gnome-firmware",
        "gnome-font-viewer",
        "gnome-maps",
        "gnome-screenshot",
        "gnome-software",
        "gnome-software-packagekit-plugin",
        "gnome-sound-recorder",
        "gnome-system-monitor",
        "gnome-themes-extra",
        "gnome-tweaks",
        "gnome-weather",
        "gvfs-nfs",
        "gvfs-smb",
        "jq",
        "kitty",
        "libreoffice-fresh-en-gb",
        "libva-mesa-driver",
        "libva-utils",
        "man-db",
        "man-pages",
        "nano",
        "nautilus",
        "nmap",
        "noto-fonts",
        "noto-fonts-cjk",
        "noto-fonts-emoji",
        "obsidian",
        "otf-font-awesome",
        "p7zip",
        "papirus-icon-theme",
        "prettyping",
        "python-internetarchive",
        "radeontop",
        "rsync",
        "seahorse",
        "signal-desktop",
        "solaar",
        "speedtest-cli",
        "starship",
        "tailscale",
        "terraform",
        "tmux",
        "tree",
        "ufw",
        "usbutils",
        "vulkan-radeon",
        "vulkan-tools",
        "wmctrl",
        "xdg-user-dirs",
        "xdg-utils",
        "zsh"
    ],
    "profile": "minimal",
    "services": [
        "bluetooth",
        "cups",
        "cups-browsed",
        "docker",
        "fail2ban",
        "gdm",
        "sshd",
        "systemd-resolved",
        "systemd-timesyncd",
        "tailscaled",
        "ufw"
    ],
    "swap": false,
    "sys-encoding": "utf-8",
    "sys-language": "en_GB",
    "timezone": "Europe/London",
    "version": "2.3.3"
}
